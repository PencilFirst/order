














/*每个页面公共css */
page,
view,
input,
text,
form,
navigator,
rich-text,
picker,
scroll-view,
cover-view,
open-data {
	box-sizing: border-box;
}
rich-text,
open-data,
form {
	display: block;
}
._del {
	color: #999;
	text-decoration: line-through;
}
image {
	display: block;
}
cover-view {
	line-height: 1.5;
	white-space: normal;
}
page {
	font-size: 28rpx;
	font-weight: 300;
	line-height: 1.5;
	color: #404040;
	background: #f8f8f8;
	font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', arial, sans-serif;
}
/* 隐藏默认滚动条 */
page,
view,
input,
text,
form,
navigator,
rich-text,
picker,
scroll-view,
cover-view,
open-data {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}
::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}

@font-face {font-family: "sell-icon";
  src: url(data:application/vnd.ms-fontobject;base64,); /* IE9 */
  src: url(data:application/vnd.ms-fontobject;base64,#iefix) format('embedded-opentype'), 
  url(data:font/woff;base64,) format('woff'),
  url(data:font/ttf;base64,) format('truetype'), 
  url(../../static/img/sell-icon.98413c3c.svg#sell-icon) format('svg'); /* iOS 4.1- */
}

.icon {
  font-family: "sell-icon" !important;
  /* font-size: 16px; */
